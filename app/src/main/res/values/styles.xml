<resources>
    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light">
        <!-- Primary theme color of the app (sets background color of action bar) -->
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <!-- Primary theme color of the app (sets background color of app bar) -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <!-- Background color of action bar in the app -->
        <item name="android:textColorPrimary">#FFFFFF</item>
        <!-- Background color of buttons in the app -->
        <item name="colorButtonNormal">@color/colorAccent</item>



    </style>

</resources>